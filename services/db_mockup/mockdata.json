      "user_profiles": 2,
      "user_settings": 2,
      "user_chats": 2,
      "quizzes": 2,
      "quiz_results": 2,
      "applications": 2,
      "followed_jobs": 20,
      "viewed_jobs": 2,
      "employee_verification_tokens": 4
    }
  }
  